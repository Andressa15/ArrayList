package br.com.zup;

import java.util.List;

public class Pessoa {

	public static void main(String[] args) {
		Pessoa yuki = new Pessoa("Vinicius, 1.75");
		Pessoa pedro = new Pessoa(" Pedro", 1,5);

		List<Pessoa> arrayList = new ArrayList<pessoa>();
		arrayList.add(yuki);
		arrayList.add(pedro);

		}
		public String toString() {
        String aux = "";
    aux += "Nome  ->" + this.nome + "\n";
        aux += "Tamanho ->" + this.tamanho;
        return aux;
		}

	}
}
